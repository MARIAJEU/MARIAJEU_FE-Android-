<?xml version="1.0" encoding="utf-8"?>


<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <TextView
            android:id="@+id/filter_price_sec1_region_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="33dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="27dp"
            android:text="가격"
            android:textSize="20sp"
            android:textStyle="bold"
            android:gravity="top"
            />

        <TextView
            android:id="@+id/filter_price_sec1_reset_tv"
            android:layout_width="34dp"
            android:layout_height="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="25dp"
            android:layout_marginTop="28dp"
            android:text="초기화"
            android:textSize="12dp" android:textColor="#B51D1D"
            android:gravity="top"
            />
        <ImageView
            android:id="@+id/filter_price_sec1_reset_iv"
            android:layout_width="12dp"
            android:layout_height="16dp"
            android:src="@drawable/ic_filter_region_reset"
            android:layout_marginTop="27dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@id/filter_price_sec1_reset_tv"
            android:layout_marginEnd="5dp"
            />

        <View
            android:id="@+id/filter_price_sec1_v"
            android:layout_width="328dp"
            android:layout_height="1dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="69dp"
            android:background="@drawable/filter_region_line_25"
            />


        <TextView
            android:id="@+id/filter_price_sec2_range_tv"
            android:layout_width="66.16dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec1_v"
            android:layout_marginTop="18dp"
            android:text="가격 범위"
            android:textSize="15dp"
            android:gravity="top"
            />

        <View
            android:id="@+id/filter_price_sec2_range_v"
            android:layout_width="299.91dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="200.58dp"
            android:background="@drawable/filter_price_line_26"
            />
        
        <ImageView
            android:id="@+id/filter_price_sec2_range_lowest_iv"
            android:layout_width="21.17dp"
            android:layout_height="21.17dp"
            android:src="@drawable/ic_filter_price_ellipse_34"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec2_range_v"
            app:layout_constraintBottom_toBottomOf="@id/filter_price_sec2_range_v"
            app:layout_constraintEnd_toEndOf="@id/filter_price_sec2_range_v"
            android:layout_marginEnd="289.5dp"
            />

        <ImageView
            android:id="@+id/filter_price_sec2_range_highest_iv"
            android:layout_width="21.17dp"
            android:layout_height="21.17dp"
            android:src="@drawable/ic_filter_price_ellipse_34"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec2_range_v"
            app:layout_constraintBottom_toBottomOf="@id/filter_price_sec2_range_v"
            app:layout_constraintStart_toStartOf="@id/filter_price_sec2_range_v"
            android:layout_marginStart="289.5dp"
            />

        <TextView
            android:id="@+id/filter_price_sec2_range_lowest_tv"
            android:layout_width="15.88dp"
            android:layout_height="10.58dp"
            app:layout_constraintStart_toStartOf="@id/filter_price_sec2_range_lowest_iv"
            app:layout_constraintEnd_toEndOf="@id/filter_price_sec2_range_lowest_iv"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec2_range_lowest_iv"
            android:layout_marginTop="5.29dp"
            android:text="0원"
            android:textAppearance="@style/filter_price_text"
            android:gravity="top"
            />

        <TextView
            android:id="@+id/filter_price_sec2_range_middle_tv"
            android:layout_width="wrap_content"
            android:layout_height="10.58dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec2_range_lowest_iv"
            android:layout_marginTop="5.29dp"
            android:text="10만원"
            android:textAppearance="@style/filter_price_text"
            android:gravity="top"
            />

        <TextView
            android:id="@+id/filter_price_sec2_range_highest_tv"
            android:layout_width="wrap_content"
            android:layout_height="10.58dp"
            app:layout_constraintStart_toStartOf="@id/filter_price_sec2_range_highest_iv"
            app:layout_constraintEnd_toEndOf="@id/filter_price_sec2_range_highest_iv"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec2_range_lowest_iv"
            android:layout_marginTop="5.29dp"
            android:text="20만원"
            android:textAppearance="@style/filter_price_text"
            android:gravity="top"
            />



        <TextView
            android:id="@+id/filter_price_sec2_range_change_from_tv"
            android:layout_width="42.37dp"
            android:layout_height="14.53dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="271dp"
            android:text="~"
            android:textSize="12.105dp"
            android:gravity="center_horizontal|top"
            />

        <TextView
            android:id="@+id/filter_price_sec2_range_change_lowest_tv"
            android:layout_width="48.42dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec2_range_change_from_tv"
            app:layout_constraintEnd_toStartOf="@id/filter_price_sec2_range_change_from_tv"
            android:layout_marginEnd="18.16dp"
            android:text="0 만원"
            android:textSize="12.105dp"
            android:gravity="center_horizontal|top"
            />

        <TextView
            android:id="@+id/filter_price_sec2_range_change_highset_tv"
            android:layout_width="58.42dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec2_range_change_from_tv"
            app:layout_constraintStart_toEndOf="@id/filter_price_sec2_range_change_from_tv"
            android:layout_marginEnd="18.16dp"
            android:text="20 만원"
            android:textSize="12.105dp"
            android:gravity="center_horizontal|top"
            />





        <View
            android:id="@+id/filter_price_sec3_v"
            android:layout_width="328dp"
            android:layout_height="1dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="556dp"
            android:background="@drawable/filter_region_line_25"
            />

        <View
            android:id="@+id/filter_price_sec3_confirm_v"
            android:layout_width="156dp"
            android:layout_height="37dp"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec3_v"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="44dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="163dp"
            android:background="@drawable/filter_region_rectangle_8"
            />
        <TextView
            android:id="@+id/filter_price_sec3_confirm_tv"
            android:layout_width="41dp"
            android:layout_height="15dp"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec3_confirm_v"
            app:layout_constraintStart_toStartOf="@id/filter_price_sec3_confirm_v"
            android:layout_marginTop="11dp"
            android:layout_marginStart="58dp"
            android:text="확인"
            android:textSize="12dp"
            android:gravity="center_horizontal|top"
            />

        <View
            android:id="@+id/filter_price_sec3_cancel_v"
            android:layout_width="156dp"
            android:layout_height="37dp"
            app:layout_constraintTop_toBottomOf="@id/filter_price_sec3_v"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="44dp"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="163dp"
            android:background="@drawable/filter_region_rectangle_9"
            />
        <TextView
            android:id="@+id/filter_price_sec3_cancel_tv"
            android:layout_width="41dp"
            android:layout_height="15dp"
            app:layout_constraintTop_toTopOf="@id/filter_price_sec3_cancel_v"
            app:layout_constraintStart_toStartOf="@id/filter_price_sec3_cancel_v"
            android:layout_marginTop="11dp"
            android:layout_marginStart="58dp"
            android:text="취소"
            android:textSize="12dp"
            android:textColor="@color/white"
            android:gravity="center_horizontal|top"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
